{
    "name": "mychannel",
    "x-type": "hlfv1",
    "version": "1.0.0",
    "tlsEnabled": true,
    "client": {
        "organization": "Org1",
        "connection": {
            "timeout": {
                "peer": {
                    "endorser": "300"
